# CTAS Frontend Environment Variables
# Copy this file to .env and fill in your actual values

# API Configuration
VITE_API_URL=https://ctas-backend-api.onrender.com/api

# Mapbox Configuration (Required for satellite maps)
VITE_MAPBOX_ACCESS_TOKEN=pk.your_mapbox_token_here

# Google Maps Configuration (If using Google Maps)
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Weather Service API Key
VITE_OPENWEATHER_API_KEY=your_openweather_api_key_here

# Application Configuration
VITE_APP_NAME="Coastal Threat Alert System"
VITE_APP_VERSION="1.0.0"

# For Development Only
# VITE_API_URL=http://localhost:8000/api