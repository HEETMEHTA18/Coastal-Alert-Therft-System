# CTAS Frontend Environment Variables
# Copy this file to .env and fill in your actual values

# API Configuration
VITE_API_URL=https://XXXXX_REPLACE_WITH_YOUR_BACKEND_URL.onrender.com/api

# Mapbox Configuration (Required for satellite maps)
# Get this from https://account.mapbox.com/access-tokens/
VITE_MAPBOX_ACCESS_TOKEN=XXXXX_REPLACE_WITH_MAPBOX_ACCESS_TOKEN

# Google Maps Configuration (If using Google Maps)
# Get this from https://console.cloud.google.com/
VITE_GOOGLE_MAPS_API_KEY=XXXXX_REPLACE_WITH_GOOGLE_MAPS_API_KEY

# Weather Service API Key
# Get this from https://openweathermap.org/api
VITE_OPENWEATHER_API_KEY=XXXXX_REPLACE_WITH_OPENWEATHER_API_KEY

# Application Configuration
VITE_APP_NAME="Coastal Threat Alert System"
VITE_APP_VERSION="1.0.0"

# For Development Only
# VITE_API_URL=http://localhost:8000/api